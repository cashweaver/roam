@article{SuccinctDataStructure2021,
  title = {Succinct Data Structure},
  year = {2021},
  month = feb,
  journal = {Wikipedia},
  url = {https://en.wikipedia.org/w/index.php?title=Succinct_data_structure&oldid=1007270369},
  urldate = {2022-06-29},
  abstract = {In computer science, a succinct data structure is a data structure which uses an amount of space that is "close" to the information-theoretic lower bound, but (unlike other compressed representations) still allows for efficient query operations.  The concept was originally introduced by Jacobson to encode bit vectors, (unlabeled) trees, and planar graphs. Unlike general lossless data compression algorithms, succinct data structures retain the ability to use them in-place, without decompressing them first. A related notion is that of a compressed data structure, in which the size of the data structure depends upon the particular data being represented. Suppose that                         Z                 \{\textbackslash displaystyle Z\}    is the information-theoretical optimal number of bits needed to store some data. A representation of this data is called: implicit if it takes                         Z         +         O         (         1         )                 \{\textbackslash displaystyle Z+O(1)\}    bits of space, succinct if it takes                         Z         +         o         (         Z         )                 \{\textbackslash displaystyle Z+o(Z)\}    bits of space, and compact if it takes                         O         (         Z         )                 \{\textbackslash displaystyle O(Z)\}    bits of space.For example, a data structure that uses                         2         Z                 \{\textbackslash displaystyle 2Z\}    bits of storage is compact,                         Z         +                                 Z                                     \{\textbackslash displaystyle Z+\{\textbackslash sqrt \{Z\}\}\}    bits is succinct,                         Z         +         lg         ⁡         Z                 \{\textbackslash displaystyle Z+\textbackslash lg Z\}    bits is also succinct, and                         Z         +         3                 \{\textbackslash displaystyle Z+3\}    bits is implicit. Implicit structures are thus usually reduced to storing information using some permutation of the input data; the most well-known example of this is the heap.},
  copyright = {Creative Commons Attribution-ShareAlike License},
  langid = {english},
  annotation = {Page Version ID: 1007270369},
  file = {/home/cashweaver/Zotero/storage/Z5DQ4NAG/Succinct_data_structure.html}
}

